{
  "extends": "ts-config-gm-react-app/tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "importsNotUsedAsValues": "preserve",
    "paths": {
      "common/*": ["src/common/*"],
      "stores/*": ["src/stores/*"],
      "svg/*": ["src/svg/*"],
      "img/*": ["src/img/*"],
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "global.d.ts"]
}
